{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Laravel Base Project",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.0",
        "guzzlehttp/guzzle": "^7.8",
        "spatie/laravel-permission": "^6.0",
        "maatwebsite/excel": "^3.1.60",
        "bacon/bacon-qr-code": "^2.0",
        "irazasyed/telegram-bot-sdk": "^3.12",
        "pusher/pusher-php-server": "^7.2",
        "league/flysystem-sftp-v3": "^3.0",
        "phpseclib/phpseclib": "^3.0",
        "riskihajar/terbilang": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "nunomaduro/collision": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Sftp.php",
            "app/Helpers/sftpbast.php",
            "app/Helpers/sftpefaktur.php",
            "app/Helpers/sftpinvoice.php",
            "app/Helpers/sftpsuratjalan.php",
            "app/Helpers/sftpkontrak.php",
            "app/Helpers/sftpkuitansi.php",
            "app/Helpers/sftptagihan.php",
            "app/Helpers/sftprealisasi.php",
            "app/Helpers/Filestorage.php",
            "app/Helpers/ResponseFormatter.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}